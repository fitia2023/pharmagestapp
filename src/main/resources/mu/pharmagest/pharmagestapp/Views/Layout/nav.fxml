<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="700.0" prefWidth="50.0" styleClass="bac" stylesheets="@../../assets/css/nav.css"
            xmlns="http://javafx.com/javafx/20" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="mu.pharmagest.pharmagestapp.Controller.Dashboard.NavController">
    <VBox alignment="CENTER" layoutY="56.0" prefWidth="50.0" spacing="20.0" styleClass="vboxnav">
        <children>
            <JFXButton fx:id="btnhome" alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false"
                       onAction="#home" styleClass="activenav" text=" ">
                <graphic>
                    <ImageView fitHeight="30.0" fitWidth="30.0" nodeOrientation="INHERIT" pickOnBounds="true"
                               preserveRatio="true">
                        <image>
                            <Image url="@../../assets/images/home.png"/>
                        </image>
                    </ImageView>
                </graphic>
            </JFXButton>
            <JFXButton fx:id="btnvente" alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false"
                       onAction="#Vente" styleClass="btnnav" text=" ">
                <graphic>
                    <ImageView fitHeight="30.0" fitWidth="30.0" nodeOrientation="INHERIT" pickOnBounds="true"
                               preserveRatio="true">
                        <image>
                            <Image url="@../../assets/images/vente.png"/>
                        </image>
                    </ImageView>
                </graphic>
            </JFXButton>
            <JFXButton fx:id="btncaisse" alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false"
                       onAction="#Caisse" styleClass="btnnav" text=" ">
