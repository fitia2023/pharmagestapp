                    </ImageView>
                </graphic>
            </JFXButton>
        </children>
        <padding>
            <Insets bottom="20.0" top="20.0"/>
        </padding>
    </VBox>
    <JFXButton alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" layoutX="10.0" layoutY="636.0"
               onAction="#Logout" styleClass="btnnav" text=" ">
        <graphic>
            <ImageView fitHeight="30.0" fitWidth="30.0" nodeOrientation="INHERIT" pickOnBounds="true"
                       preserveRatio="true">
                <image>
                    <Image url="@../../assets/images/logout.gif"/>
                </image>
            </ImageView>
        </graphic>
    </JFXButton>

</AnchorPane>
