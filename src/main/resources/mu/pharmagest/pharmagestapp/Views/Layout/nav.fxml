                    </ImageView>
                </graphic>
            </JFXButton>
            <JFXButton fx:id="btnmedic" alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false"
                       onAction="#Medic" styleClass="btnnav" text=" ">
                <graphic>
                    <ImageView fitHeight="30.0" fitWidth="30.0" nodeOrientation="INHERIT" pickOnBounds="true"
                               preserveRatio="true">
                        <image>
                            <Image url="@../../assets/images/medicament.png"/>
                        </image>
                    </ImageView>
                </graphic>
            </JFXButton>
            <JFXButton fx:id="btnfourni" alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false"
                       onAction="#Fourni" styleClass="btnnav" text=" ">
                <graphic>
                    <ImageView fitHeight="30.0" fitWidth="30.0" nodeOrientation="INHERIT" pickOnBounds="true"
                               preserveRatio="true">
                        <image>
                            <Image url="@../../assets/images/fournisseur.png"/>
                        </image>
                    </ImageView>
                </graphic>
            </JFXButton>
            <JFXButton fx:id="btnappro" alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false"
                       onAction="#Appro" styleClass="btnnav" text=" ">
                <graphic>
                    <ImageView accessibleRole="BUTTON" fitHeight="30.0" fitWidth="30.0" nodeOrientation="INHERIT"
                               pickOnBounds="true" preserveRatio="true">
                        <image>
                            <Image url="@../../assets/images/entrepot.png"/>
                        </image>
                    </ImageView>
                </graphic>
            </JFXButton>
            <JFXButton fx:id="btnlivraison" alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false"
                       onAction="#Livraison" styleClass="btnnav" text=" ">
                <graphic>
                    <ImageView fitHeight="30.0" fitWidth="30.0" nodeOrientation="INHERIT" pickOnBounds="true"
                               preserveRatio="true">
                        <image>
                            <Image url="@../../assets/images/boite.png"/>
                        </image>
                    </ImageView>
                </graphic>
            </JFXButton>
            <JFXButton fx:id="btnutilisateur" alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false"
                       onAction="#Utilisateur" styleClass="btnnav" text=" ">
                <graphic>
                    <ImageView fitHeight="30.0" fitWidth="30.0" nodeOrientation="INHERIT" pickOnBounds="true"
